{
  "name": "tellstick-adapter",
  "version": "0.0.1",
  "description": "Telldus Tellstick adapter add-on for Mozilla WebThings Gateway",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --listEmittedFiles",
    "lint": "eslint --ext .ts src",
    "test": "mocha \"test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/majori/tellstick-adapter.git"
  },
  "author": "Antti Kivim√§ki",
  "license": "MIT",
  "keywords": [
    "mozilla",
    "iot",
    "adapter",
    "tellstick",
    "telldus"
  ],
  "bugs": {
    "url": "https://github.com/majori/tellstick-adapter/issues"
  },
  "homepage": "https://github.com/majori/tellstick-adapter#readme",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gateway-addon": "git+https://github.com/mozilla-iot/gateway-addon-node.git",
    "mocha": "^6.2.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.2"
  },
  "dependencies": {},
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "dist/constants.js",
    "dist/index.js",
    "dist/core.js",
    "dist/tellstick-adapter.js"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "config": {
      "socket": "/tmp/TelldusClient"
    },
    "schema": {
      "type": "object",
      "properties": {
        "socket": {
          "type": "string"
        }
      }
    }
  }
}
